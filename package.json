{
  "name": "nuxthub",
  "version": "0.5.1",
  "license": "Apache-2.0",
  "description": "Interface with the NuxtHub platform from the command line.",
  "keywords": [
    "nuxt",
    "nuxthub",
    "cli"
  ],
  "repo": "nuxt-hub/cli",
  "type": "module",
  "bin": "./src/index.mjs",
  "files": [
    "src/"
  ],
  "scripts": {
    "lint": "eslint --ext .mjs .",
    "nuxthub": "./src/index.mjs",
    "prepublishOnly": "pnpm lint",
    "release": "release-it"
  },
  "dependencies": {
    "@clack/prompts": "^0.7.0",
    "blake3-wasm": "^2.1.5",
    "citty": "^0.1.6",
    "consola": "^3.2.3",
    "dotenv": "^16.4.5",
    "execa": "^8.0.1",
    "get-port-please": "^3.1.2",
    "h3": "^1.11.1",
    "is-docker": "^3.0.0",
    "listhen": "^1.7.2",
    "load-json-file": "^7.0.1",
    "mime": "^4.0.1",
    "ofetch": "^1.3.3",
    "open": "^10.0.4",
    "pathe": "^1.1.2",
    "pretty-bytes": "^6.1.1",
    "rc9": "^2.1.1",
    "ufo": "^1.4.0",
    "unstorage": "^1.10.1",
    "update-notifier": "^7.0.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^0.2.0",
    "eslint": "^8.57.0",
    "release-it": "^17.1.1"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore(release): release v${version}"
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    }
  }
}
